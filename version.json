{
  "version": "1.0-prerelease", 

  "nugetPackageVersion": {
     "semVer": 1 
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$", 
    "^refs/tags/v\\d+\\.\\d+"
  ],
  "cloudBuild": {
    "setVersionVariables": true,
    "buildNumber": {
      "enabled": false
    }
  },
  "release" : {
    "branchName" : "v{version}",
    "versionIncrement" : "minor",
    "firstUnstableTag" : "alpha"
  },
  "inherit": false 
}